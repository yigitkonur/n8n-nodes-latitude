{
  "name": "n8n-nodes-latitude",
  "version": "0.3.4",
  "description": "n8n community node for Latitude.so - AI prompt management and execution platform",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "latitude",
    "ai",
    "llm",
    "prompts"
  ],
  "license": "MIT",
  "homepage": "https://github.com/yigitkonur/n8n-nodes-latitude#readme",
  "author": {
    "name": "Yigit Konur",
    "email": "yigit35@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yigitkonur/n8n-nodes-latitude.git"
  },
  "bugs": {
    "url": "https://github.com/yigitkonur/n8n-nodes-latitude/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@9.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" package.json",
    "lintfix": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/LatitudeApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Latitude/Latitude.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@latitude-data/sdk": "^5.2.2"
  }
}
